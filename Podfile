target 'UniVibe' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '16.4'
  
  # Pods for UniVibe
  # see tutorial: https://firebase.google.com/docs/ios/setup
  # also see: https://github.com/firebase/firebase-ios-sdk
  
  pod 'FirebaseCore'
  pod 'FirebaseFirestore'
  pod 'FirebaseAuth'
  pod 'FirebaseStorage'
  pod 'FirebaseFirestoreSwift'
  pod 'Firebase'
  pod 'FirebaseAnalytics'
  pod 'FirebaseInstallations'
  pod 'GoogleAppMeasurement'
  
end
